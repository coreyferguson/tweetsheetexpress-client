  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run html && npm run webpack",
    "clean": "rm -fr dist",
    "html": "mkdir -p dist && cp src/*html dist/",
    "webpack": "./node_modules/.bin/webpack",
    "deploy": "npm run build && node scripts/deploy",
    "test": "node_modules/.bin/karma start test/karma.conf.js --single-run",
    "testManual": "npm run clean && npm run html && NODE_ENV=local node_modules/.bin/webpack-dev-server --debug --output-pathinfo --public local.tweetsheets-dev.overattribution.com:3000 --https",
    "watch": "node_modules/.bin/karma start test/karma.conf.js --no-single-run"
  },
